<?xml version="1.0" encoding="UTF-8"?>
<odoo>

<record model="ir.ui.view" id="dummy_zip_form">
  <field name="name">dummy.zip.form</field>
  <field name="model">dummy.zip</field>
  <field name="arch" type="xml">
    <form string="Dummy">
      <sheet>
        <group>
          <group>
            <field name="name"/>
            <field name="city"/>
            <field name="priority" />
          </group>
        </group>
        <notebook colspan="4">
          <page string="Geometry">
            <field name="the_geom" colspan="4" widget="geo_edit_map" options='{"opacity": 0.4, "fill_color":"#33ccff"}'/>
          </page>
        </notebook>
        <field name="total_sales"/>
      </sheet>
    </form>
  </field>
</record>
<record model="ir.ui.view" id="dummy_zip_tree">
  <field name="name">dummy.zip.form</field>
  <field name="model">dummy.zip</field>
  <field name="arch" type="xml">
    <tree string="Dummy">
      <field name="name"/>
      <field name="city"/>
      <field name="priority"/>
      <field name="total_sales"/>
    </tree>
  </field>
</record>
<record model="ir.ui.view" id="dummy_zip_search">
  <field name="name">dummy.zip.search</field>
  <field name="model">dummy.zip</field>
  <field name="arch" type="xml">
    <search string="Search ZIP">
      <field name="name"/>
      <field name="city"/>
    </search>
  </field>
</record>
<record id="action_zip_tree" model="ir.actions.act_window">
  <field name="name">ZIP Management</field>
  <field name="res_model">dummy.zip</field>
  <field name="view_type">form</field>
  <field name="view_mode">tree,form</field>
  <field eval="dummy_zip_tree" name="view_id"/>
</record>
<!-- GeoEngine views -->
<record id="ir_ui_view_resbetterzipgeoview0" model="ir.ui.view">
  <field name="name">dummy.zip.geo_view</field>
  <field name="arch" type="xml">
    <geoengine version="7.0">
      <field name="name"/>
      <field name="city"/>
      <field name="total_sales"/>
      <templates>
        <t t-name="layer-box">
            <div id="content">
                <b>
                    <field name="name"/> / <field name="city"/>
                </b>
                <hr/>
                <div class="row">
                    <div class="col-xs-4">
                        <b>Total sales:</b>
                    </div>
                    <div class="col-xs-8">
                        <field name="total_sales"/>
                    </div>
                </div>
            </div>
        </t>
      </templates>
    </geoengine>
  </field>
  <field eval="16" name="priority"/>
  <field name="model">dummy.zip</field>
</record>

<record id="retail_machine_details_view_geoengine" model="ir.ui.view">
  <field name="name">retail.machine.details.geo_view</field>
  <field name="arch" type="xml">
    <geoengine>
      <field name="name"/>
      <field name="total_sales"/>
      <field name="money_level"/>
      <templates>
        <t t-name="layer-box">
            <div id="content">
                <b>
                    <field name="name"/>
                </b>
                <hr/>
                <div class="row">
                    <div class="col-xs-4">
                        <b>Total sales:</b>
                    </div>
                    <div class="col-xs-8">
                        <field name="total_sales"/>
                    </div>
                </div>
                <br/>
                <div class="row">
                    <div class="col-xs-4">
                        <b>Money level:</b>
                    </div>
                    <div class="col-xs-8">
                        <field name="money_level"/>
                    </div>
                </div>
            </div>
        </t>
      </templates>
    </geoengine>
  </field>
  <!--
  put a higher priority to be sure that this view is not the one used by default
  -->
  <field name="priority" eval="99"/>
  <field name="model">geoengine.demo.automatic.retailing.machine</field>
</record>
<record id="geoengine_vector_layer_basictest0" model="geoengine.vector.layer">
  <field name="geo_field_id" ref="base_geoengine_demo.field_dummy_zip_the_geom"/>
  <field name="name">Basic ZIP</field>
  <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
  <field name="geo_repr">basic</field>
  <field name="attribute_field_id" ref="field_dummy_zip_city"/>
  <field name="display_polygon_labels" eval="0"/>
  <field name="begin_color">#FF680A</field>
  <field name="layer_opacity">0.3</field>
</record>


<record id="geoengine_vector_layer_all_retail_machines" model="geoengine.vector.layer">
  <field name="model_id" ref="base_geoengine_demo.model_geoengine_demo_automatic_retailing_machine" />
  <field name="model_view_id" ref="base_geoengine_demo.retail_machine_details_view_geoengine"/>
  <field name="geo_field_id" ref="base_geoengine_demo.field_geoengine_demo_automatic_retailing_machine_the_point"/>
  <field name="name">All retail machines</field>
  <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
  <field name="geo_repr">basic</field>
  <field name="begin_color">#0aa9ff</field>
  <field name="display_polygon_labels" eval="0"/>
  <field name="layer_opacity">0.9</field>
</record>

<record id="geoengine_vector_layer_hs_retail_machines" model="geoengine.vector.layer">
  <field name="model_id" ref="base_geoengine_demo.model_geoengine_demo_automatic_retailing_machine" />
  <field name="model_view_id" ref="base_geoengine_demo.retail_machine_details_view_geoengine"/>
  <field name="model_domain">[("state", "=", "hs")]</field>
  <field name="geo_field_id" ref="base_geoengine_demo.field_geoengine_demo_automatic_retailing_machine_the_point"/>
  <field name="name">HS retail machines</field>
  <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
  <field name="geo_repr">basic</field>
  <field name="begin_color">#FF680A</field>
  <field name="display_polygon_labels" eval="0"/>
  <field name="layer_opacity">0.9</field>
</record>

<record id="geoengine_vector_layer_coloredrangetest0" model="geoengine.vector.layer">
  <field name="geo_field_id" ref="base_geoengine_demo.field_dummy_zip_the_geom"/>
  <field name="name">Colored range ZIP</field>
  <field name="classification">unique</field>
  <field eval="6" name="sequence"/>
  <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
  <field name="geo_repr">colored</field>
  <field eval="1" name="nb_class"/>
  <field name="attribute_field_id" ref="base_geoengine_demo.field_dummy_zip_total_sales"/>
  <field name="begin_color">#FF680A</field>
</record>
<record id="geoengine_vector_layer_coloredrangezipquantile0" model="geoengine.vector.layer">
  <field name="geo_field_id" ref="base_geoengine_demo.field_dummy_zip_the_geom"/>
  <field name="name">Colored range ZIP Eq Interval</field>
  <field name="classification">interval</field>
  <field eval="6" name="sequence"/>
  <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
  <field name="geo_repr">colored</field>
  <field eval="4" name="nb_class"/>
  <field name="attribute_field_id" ref="base_geoengine_demo.field_dummy_zip_total_sales"/>
  <field name="begin_color">#FF680A</field>
  <field name="end_color">#00680A</field>
</record>
<record id="geoengine_raster_layer_test0" model="geoengine.raster.layer">
  <field name="raster_type">osm</field>
  <field name="name">ZIP OSM</field>
  <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
  <field eval="0" name="overlay"/>
</record>
<record id="geoengine_raster_layer_osm0" model="geoengine.raster.layer">
  <field name="raster_type">osm</field>
  <field name="name">ZIP OSM Light</field>
  <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
  <field eval="1" name="overlay"/>
</record>

</odoo>
