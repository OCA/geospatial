<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
         <record id="view_ceom_in_zip" model="ir.ui.view">
                <field name="name">better.zip.geom</field>
                <field name="model">res.better.zip</field>
                <field name="type">form</field>
                <field name="inherit_id" ref="better_zip.better_zip_form" />
                <field name="arch" type="xml">
                    <field name="city" position="after">
                       <field name="the_geom" colspan="4" widget="text"/>
                       <field name="total_sales"/>
                       
                    </field>
                </field>
         </record>
        <!-- GEOEngine view -->
        <record id="ir_ui_view_resbetterzipgeoview0" model="ir.ui.view">
            <field name="name">res.better.zip.geo_view</field>
            <field name="arch"><![CDATA[ 
                <form >
                    <field name="name"/>
                    <field name="city"/>
                </form>]]></field>
            <field eval="16" name="priority"/>
            <field name="type">geoengine</field>
            <field name="model">res.better.zip</field>
        </record>
        <record id="geoengine_vector_layer_basictest0" model="geoengine.vector.layer">
            <field name="geo_field_id" ref="base_geoengine_demo.field_res_better_zip_the_geom"/>
            <field name="name">Basic test</field>
            <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
            <field name="geo_repr">basic</field>
            <field name="attribute_field_id" ref="better_zip.field_res_better_zip_city"/>
            <field name="begin_color">#FF680A</field>
        </record>
        <record id="geoengine_vector_layer_coloredrangetest0" model="geoengine.vector.layer">
            <field name="geo_field_id" ref="base_geoengine_demo.field_res_better_zip_the_geom"/>
            <field name="name">Colored range Test</field>
            <field name="classification">unique</field>
            <field eval="6" name="sequence"/>
            <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
            <field name="geo_repr">colored</field>
            <field eval="1" name="nb_class"/>
            <field name="attribute_field_id" ref="better_zip.field_res_better_zip_name"/>
            <field name="begin_color">#FF680A</field>
        </record>
        <record id="geoengine_raster_layer_test0" model="geoengine.raster.layer">
            <field name="raster_type">osm</field>
            <field name="name">test</field>
            <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
            <field eval="0" name="overlay"/>
        </record>
        <record id="geoengine_raster_layer_googletest0" model="geoengine.raster.layer">
            <field name="raster_type">google</field>
            <field name="google_type">G_HYBRID_MAP</field>
            <field name="name">Google test</field>
            <field name="view_id" ref="ir_ui_view_resbetterzipgeoview0"/>
            <field eval="1" name="overlay"/>
        </record>
    </data>
</openerp>

